local EggTab = Window:CreateTab("Eggs", nil) -- Title, Image
local EggSection = EggTab:CreateSection("Eggs")

local Toggle = EggTab:CreateToggle({
    Name = "Zone 1 Egg",
    CurrentValue = false,
    Flag = "Toggle12",
    Callback = function(Value)
        _G.ToggleValue = Value
        if _G.ToggleValue then
            while _G.ToggleValue do
                local args = {
                    [1] = "Single",
                    [2] = "Z1",
                    [3] = false,
                    [4] = {},
                    [5] = false
                }
                game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("petHatching"):FireServer(unpack(args))
                wait() -- Wait before sending the request again
            end
        end
    end,
})

